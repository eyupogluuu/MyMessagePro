    <!--scrolling js-->
    <script src="~/web/js/jquery.nicescroll.js"></script>
    <script src="~/web/js/scripts.js"></script>
    <!--//scrolling js-->
    <script src="~/web/js/bootstrap.js"></script>
    <!-- mother grid end here-->
</body>
</html>





